// Dealing with MIDI data

void doNoteOn(byte channel, byte pitch, byte velocity) {   
  
  if(channel == 0 && pitch < 16) {
    SPB_sound = pitch;
    if(velocity > 0) {
      writeSN76489(B10000001, 0); 
      writeSN76489(B00000000, 0); 
      writeSN76489(B10100001, 0);
      writeSN76489(B00000000, 0); 
      writeSN76489(B11000001, 0);
      writeSN76489(B00000000, 0);
      
      SPB_speed = 127 - velocity;
      SPB_flag = 1;
      SPB_counter = 0;
      SPB_max = sample_length_list[SPB_sound + 1] - sample_length_list[SPB_sound];
    }
    else {
      SPB_flag = 0;
    }
  }
        
else {
  
  
  byte chip_select = channel / 4; 
  channel = channel % 4; 
  
  
  // NORMAL PULSE CHANNELS
  if(channel >= 0 && channel <= 2) {
    
    if(channel == 2 && bass_mode == 1) {
    {
      velocity = velocity / 3;
    }
    
    
    // writeSN76489(B11100011); // clock by frequency
    
    if(velocity > 0) {
      int coarsePitchVal = -12 + (coarsePitch[channel] / 5); 
      pitchData[channel + (chip_select * 4)] = pitch;
      velocityData[channel + (chip_select * 4) + 1] = velocity;
      writeFrequency(pitch, channel, chip_select);
      writeAmplitude(velocity, channel + 1, chip_select);
    }

    else {
      velocityData[channel + 1 + (chip_select * 4)] = 0;
      writeAmplitude(0, channel + 1, chip_select);
    }
    
    channel = 17; 
  }
  
  else{
    
    
    if(velocity > 0) {
      int coarsePitchVal = -12 + (coarsePitch[channel + (chip_select * 4)] / 5); 
      pitchData[channel + (chip_select * 4)] = pitch;
      velocityData[channel + (chip_select * 4)] = velocity;
      writeFrequency(pitch, channel, chip_select);
      writeAmplitude(velocity, channel, chip_select);
    }

    else {
      velocityData[channel + (chip_select * 4)] = 0;
      writeAmplitude(0, channel, chip_select);
    }
  }
  }

  // NOISE
  else if(channel == 3) {
    
    if(velocity > 0) {
      velocityData[channel + (chip_select * 4)] = velocity;
      writeSN76489(noiseLookup[pitch % 12], chip_select);
      writeAmplitude(velocity, channel, chip_select);
    }

    else {
      velocityData[channel + (chip_select * 4)] = 0;
      writeAmplitude(0, channel, chip_select);
    }
  }   
        }
}


void doNoteOff(byte channel, byte pitch, byte velocity) {
  if(channel == 0 && pitch < 16) {
    SPB_flag = 0;
  }
  else {
  
  byte chip_select = channel / 4; 
  channel = channel % 4;
    velocityData[channel + (chip_select * 4)] = 0;
    writeAmplitude(0, channel, chip_select);
  }
}



void doCC(byte channel, byte ccnumber, byte ccvalue) {
  byte chip_select = channel / 4; 
  channel = channel % 4; 
  
  if(ccnumber == 83 && channel == 0) {
    if(ccvalue > 63) {
      pitchTableOffset = 83;
    }
    else{
      pitchTableOffset = 0;
    }
  }

  // MIDI CC 42 = coarse tuning
  else if(ccnumber == 42 && channel >= 0 && channel <= 3) {
    coarsePitch[channel + (chip_select * 4)] = ccvalue; 
    writeFrequency(pitchData[channel + (chip_select * 4)], channel, chip_select);
  }



  // MIDI CC 70 = AM On
  else if(ccnumber == 70 && channel >= 0 && channel <= 3) {
    amModOn[channel + (chip_select * 4)] = ccvalue >> 6; 
  }

  // MIDI CC 18 = AM Speed
  else if(ccnumber == 18 && channel >= 0 && channel <= 3) {
    amSpeed[channel + (chip_select * 4)] = ccvalue; 
  }

  // MIDI CC 19 = AM Depth
  else if(ccnumber == 19 && channel >= 0 && channel <= 3) {
    amDepth[channel + (chip_select * 4)] = ccvalue; 
  }

  // MIDI CC 71 = Vib On
  else if(ccnumber == 71 && channel >= 0 && channel <= 2) {
    vibModOn[channel + (chip_select * 4)] = ccvalue >> 6; 
  }

  // MIDI CC 20 = Vib Speed
  else if(ccnumber == 20 && channel >= 0 && channel <= 2) {
    vibSpeed[channel + (chip_select * 4)] = ccvalue; 
  }

  // MIDI CC 21 = Vib Depth
  else if(ccnumber == 21 && channel >= 0 && channel <= 2) {
    vibDepth[channel + (chip_select * 4)] = ccvalue; 
  }

  // MIDI CC 11 = expression
  else if(ccnumber == 11 && channel >= 0 && channel <= 3) {
    velocityData[channel + (chip_select * 4)] = ccvalue;
    writeAmplitude(ccvalue, channel, chip_select);
  }
  
  else if(ccnumber == 81 && channel >= 0 && channel <= 3) {
    bendamount = ccvalue; 
  }

  // MIDI CC 78 = sample mode on
  
    // MIDI CC 86 = sample playback speed
  
      // MIDI CC 85 = bass mode
  else if(ccnumber == 85) {
    bass_mode = ccvalue >> 6;
  }
  
  }



void doBend(byte channel, unsigned int bend_usb) {
  byte chip_select = channel / 4; 
  channel = channel % 4;
  
  bendMSB = bend_usb >> 7; 
  bendLSB = bend_usb % B01111111; 
  


  if(channel >=0 && channel <= 2) {
    bend_MSB[channel + (chip_select * 4)] = bendMSB;
    bend_data[channel + (chip_select * 4)] = bend_MSB[channel + (chip_select * 4)];
    writeFrequency(pitchData[channel + (chip_select * 4)], channel, chip_select);
  }

}






